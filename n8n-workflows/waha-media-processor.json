        "binaryData": true,
        "additionalFields": {
          "contentType": "={{ $json.payload.type === 'image' ? 'image/jpeg' : $json.payload.type === 'video' ? 'video/mp4' : $json.payload.type === 'audio' || $json.payload.type === 'ptt' ? 'audio/ogg' : 'application/octet-stream' }}",
          "metadata": {
            "metadataValues": [
