      "id": "upload-to-s3",
      "name": "Upload para S3 IDrive",
      "type": "n8n-nodes-base.aws",
      "typeVersion": 1.1,
      "position": [900, 180],
      "credentials": {
        "aws": {
          "id": "idrive-s3-credentials",
          "name": "IDrive S3"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "const item = $input.first();\nconst s3Data = item.json;\nconst originalData = items[0].json;\n\n// Construir URL pública do S3\nconst s3Endpoint = 'https://c1k7.va.idrivee2-46.com';\nconst bucket = 'backupsistemaonenexus';\nconst fileName = s3Data.Key || s3Data.fileName;\nconst s3Url = `${s3Endpoint}/${bucket}/${fileName}`;\n\nconsole.log('✅ Mídia no S3:', s3Url);\n\n// Preparar payload para o backend\nconst payload = {\n  sessionName: originalData.session,\n  phoneNumber: originalData.payload.from.replace(/@c\\.us|@lid|@s\\.whatsapp\\.net/g, ''),\n  contactName: originalData.payload._data?.Info?.PushName || \n                originalData.payload._data?.notifyName || \n                originalData.payload.from.replace(/@c\\.us|@lid|@s\\.whatsapp\\.net/g, ''),\n  direction: originalData.payload.fromMe ? 'outgoing' : 'incoming',\n  messageType: originalData.payload.type || 'text',\n  content: originalData.payload.body || '',\n  mediaUrl: s3Url, // URL do S3 ao invés da URL temporária do WhatsApp\n  wahaMessageId: originalData.payload.id,\n  status: 'received',\n  timestamp: originalData.payload.timestamp * 1000,\n  rawPayload: originalData\n};\n\nreturn { json: payload };"
      },
