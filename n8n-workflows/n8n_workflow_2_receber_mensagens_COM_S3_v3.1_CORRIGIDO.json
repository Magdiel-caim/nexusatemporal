        "bucketName": "backupsistemaonenexus",
        "fileName": "=whatsapp/{{ $json.sessionName }}/{{ $now.toFormat('yyyyMMdd-HHmmss') }}-{{ $json.wahaMessageId }}.{{ $json.messageType === 'image' ? 'jpg' : $json.messageType === 'video' ? 'mp4' : $json.messageType === 'audio' || $json.messageType === 'ptt' ? 'ogg' : $json.messageType === 'sticker' ? 'webp' : 'bin' }}",
        "binaryPropertyName": "mediaFile",
        "additionalFields": {
          "contentType": "={{ $json.messageType === 'image' ? 'image/jpeg' : $json.messageType === 'video' ? 'video/mp4' : $json.messageType === 'audio' || $json.messageType === 'ptt' ? 'audio/ogg' : $json.messageType === 'sticker' ? 'image/webp' : 'application/octet-stream' }}",
          "metadata": {
            "metadataValues": [
              {
                "name": "source",
                "value": "whatsapp"
              },
